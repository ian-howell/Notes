DS LinkedList

template<typename T>
class LinkedList
{
private:
    T m_data;
    LinkedList* m_next;

public:
    int size() const
    {
        int counter = 0;
        const LinkedList* r = this;
        while (r->m_data != null)
        {
            counter++;
            r = r->m_next;
        }

        return counter;
    }
